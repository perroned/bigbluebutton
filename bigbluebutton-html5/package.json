    "react-addons-pure-render-mixin": "^15.0.1",
    "react-dom": "^15.0.1"
  },
  "devDependencies": {
    "coffeelint": "^1.6.0",
    "grunt": "^0.4.5",
    "jscs": "~2.11.0",
    "grunt-jscs": "~2.8.0",
    "grunt-shell": "~1.2.1",
    "load-grunt-tasks": "~3.4.1",
    "grunt-force-task": "~2.0.0",
    "grunt-contrib-watch": "~1.0.0",
    "grunt-concurrent": "~2.2.1"
  },
  "cssModules": {
    "extensions": ["scss"]
